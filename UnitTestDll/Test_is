using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

public class Test_is
{
    private static List<IBase> list;
    public static void Testis() {
        list = new List<IBase>();
        c1 _c1 = new c1();
        list.Add(_c1);

        c2 _c2 = new c2();
        list.Add(_c2);


        for (int i = 0; i < list.Count;i++ )
        {
            if (list[i] is c1) {
                System.Console.WriteLine("c1");
            }
            else if (list[i] is c2)
            {
                System.Console.WriteLine("c2");
            }
        }
    }
}

public class IBase {
    public int id;
}

public class c1 : IBase { 
    
}

public class c2 : IBase { 
    
}

